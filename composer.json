{
  "name": "dpdconnect/php-sdk",
  "type": "library",
  "description": "DPD Connect PHP SDK",
  "license": "OSL-3.0",
  "authors": [
    {
      "name": "DPD",
      "email": "pluginsupport@dpd.nl",
      "homepage": "http://www.dpd.nl"
    }
  ],
  "support": {
    "email": "pluginsupport@dpd.nl",
    "issues": "http://www.dpd.nl"
  },
  "autoload": {
    "psr-4": {
      "DpdConnect\\Sdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DpdConnect\\Sdk\\Test\\": "tests/"
    }
  },
  "require": {
    "php": "^5.5 || ^7.0 || ^8.0",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "symfony/yaml": "^4.2",
    "donatj/mock-webserver": "^2.0",
    "overtrue/phplint": "^1.1",
    "squizlabs/php_codesniffer": "^3.0@dev",
    "phpunit/phpunit": "^8.5"
  },
  "config": {
    "bin-dir": "bin"
  }
}
